mutation CreateAnEvent {
  createEvent(data: {
    board: { connect: "296192241634902532"}
    userDoing: { connect: "296192241634902532" }
    userReceiving: { connect: "296192232034140674" }
    count: 7
  }) {
    board { title }
    userDoing { displayName }
    userReceiving  { displayName }
    _id
   count
  }
}

