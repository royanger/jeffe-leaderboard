mutation CreateABoard {
  createBoard(data: {
    title: "Car Killer!"
    description: "Let's track how many times car in PUBG is used to kill another player."
    config: "user"
    action: "ran over"
    nonUser: "sadfsadf"
    user: { connect: "296192241634902532" }
  }) {
    title
    description
    config
    action
    nonUser
    user { displayName }

    _id

  }
}

